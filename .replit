modules = ["java-graalvm22.3", "postgresql-16"]

[nix]
channel = "stable-25_05"
packages = ["tree"]

[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Spring Boot Server"

[[workflows.workflow]]
name = "Spring Boot Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "./mvnw spring-boot:run"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33021
externalPort = 3001

[[ports]]
localPort = 33459
externalPort = 4200

[[ports]]
localPort = 35729
externalPort = 3000

[[ports]]
localPort = 37301
externalPort = 3003

[[ports]]
localPort = 39569
externalPort = 3002

[deployment]
deploymentTarget = "autoscale"
run = ["./mvnw", "spring-boot:run"]
